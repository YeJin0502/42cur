printf("-->|%-15p|<--\n", p);//>-->|0x93b1402710   |<--
printf("--//>|%-16.p|<--\n", p);//>-->|0x93b1402710    |<--
printf("-->|%-16p|<--\n", p);//>-->|0x93b1402710    |<--
printf("--//>|%-16.p|<--\n", p);//>-->|0x              |<--
printf("--//>|%-16.p|<--\n", p);//>-->|                |<--
printf("--//>|%-15.p|<--\n", p);//>-->|               |<--
printf("-->|%-15p|<--\n", p);//>-->|0x0            |<--
printf("--//>|%-16.p|<--\n", p);//>-->|0x              |<--
printf("--//>|%-16.p|<--\n", p);//>-->|                |<--
printf("-->|%-16p|<--\n", p);//>-->|0x0             |<--